{
	"name": "seppzzz/silverstripe-phonelink",
	"description": "Adds a telephone number option to TinyMCE's Insert Link dropdown for creating/editing tel: links using native SilverStripe 4 modals",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/seppzzz/silverstripe-phonelink",
	"keywords": [
		"silverstripe",
		"cms",
		"tinymce"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Marc Espiard",
			"email": "marc@firebrand.nz"
		},
		{
			"name": "Firebrand",
			"homepage": "https://firebrand.nz",
			"email": "developers@firebrand.nz"
		}
	],
	"require": {
		"silverstripe/cms": "^4.4"
	},
	"autoload": {
		"psr-4": {
			"seppzzz\\PhoneLink\\": "src/"
		}
	},
	"extra": {
		"expose": [
			"client/dist",
			"client/src"
		]
	},
	"prefer-stable": true,
	"minimum-stability": "stable"
}
